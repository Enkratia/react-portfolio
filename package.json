    "overlayscrollbars-react": "^0.5.1",
    "qs": "^6.11.2",
    "rc-slider": "^10.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-imask": "^6.6.3",
    "react-paginate": "^8.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^10.0.4",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",

{
  "name": "react-website-portfolio",
  "private": true,
  "version": "1.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "concurrently": "concurrently \"npm run server\" \"npm run dev\"",
    "start": "cross-env NODE_ENV=development webpack serve --hot only --open",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "icons": "npx @svgr/cli src/assets/img/icons --out-dir src/iconComponents --icon --typescript",
    "server": "json-server --watch server/db.json -m ./node_modules/json-server-auth"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.5.0",
    "decimal.js": "^10.4.3",

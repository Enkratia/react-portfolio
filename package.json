    "@types/node": "^20.4.1",
    "@types/qs": "^6.9.8",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-slick": "^0.23.10",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "copy-webpack-plugin": "^11.0.0",
